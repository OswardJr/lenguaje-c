#include <stdio.h>
int main(){

int num,i,ant1=0,ant2=1,fibo=0;

    printf("introduzca el numero que quiere saber si es fibonacci: ");
        scanf("%d",&num);

    for (i=0; i <= fibo; i++){

        fibo=ant1 + ant2;
        ant1=ant2;
        ant2=fibo;
        printf("%d ,",fibo);
       }
        if (num==ant2)
            printf("pertenece a la serie");

            else
            printf("No pertenece a la serie");

    return 0;
}
